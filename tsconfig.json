{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "es2020",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false, //CHANGED TO FALSE, TEMPORARY BECAUSE SUBGRAPH DOWN
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false, // NEED TO MAKE TRUE, SUBGRAPH RELATED
    "jsx": "preserve",
    "incremental": true,
    "typeRoots": ["node_modules/@types", "src/@types"],
    "noEmitOnError": false, // NEED TO DELETE THIS, TEMPORARY BECAUSE SUBGRAPH DOWN
    "noImplicitAny": false // NEED TO DELETE THIS, TEMPORARY BECAUSE SUBGRAPH DOWN
  },
  "include": ["src", "scripts"],
  "exclude": ["node_modules"]
}
