    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react-error-overlay": "6.0.9",
    "react-test-renderer": "^17",
    "tailwindcss": "3.2.3",
    "ts-prune": "^0.10.3",
    "typescript": "4.7.4",
    "yargs": "^17.6.2"
  }
}
